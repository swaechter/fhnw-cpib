program intDivMain
global
  const m:int32;
  const n:int32;
  var q:int32;
  var r:int32
do
  debugin m init;
  debugin n init;
  q init := 0;
  r init := m;
  while r >= n do
    q := q + 1;
    r := r - n
  endwhile;
  debugout q;
  debugout r
endprogram

